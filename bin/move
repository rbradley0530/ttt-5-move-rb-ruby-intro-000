#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Code your CLI Here

board = [" "," "," "," "," "," "," "," "," "]

puts "Welcome to Tic Tac Toe!"

puts "Where would you like to go?"

puts "* Enter a number between 1 & 9"

input = gets.strip

index = input_to_index(input)

move(board, index,)

puts "#{display_board(board)}"
